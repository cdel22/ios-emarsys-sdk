platform :ios, '11.0'
use_frameworks!

source 'https://github.com/CocoaPods/Specs.git'

target "Emarsys-Sample" do
  pod 'GoogleSignIn'
  if ENV["DEV"] then
    pod 'EmarsysSDK', :path => '../'
  else
    pod 'EmarsysSDK'
  end
end

target "NotificationService" do
  if ENV["DEV"] then
    pod 'EmarsysNotificationService', :path => '../'
  else
    pod 'EmarsysNotificationService'
  end
end
